{
    "interface_name": "ai.clea.examples.Accelerometer",
    "version_major": 0,
    "version_minor": 1,
    "type": "datastream",
    "ownership": "device",
    "aggregation": "object",
    "mappings": [
        {
            "endpoint": "/%{sensor_id}/x",
            "type": "double",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 28800,
            "explicit_timestamp": true
        },
        {
            "endpoint": "/%{sensor_id}/y",
            "type": "double",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 28800,
            "explicit_timestamp": true
        },
        {
            "endpoint": "/%{sensor_id}/z",
            "type": "double",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 28800,
            "explicit_timestamp": true
        }
    ]
}
