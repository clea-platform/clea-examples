{
  "name": "PredictiveMaintenanceVentilationSystem",
  "version": "0.1.0",
  "description": "SECO POC frontend for the predictive maintenance ventilation system.",
  "author": "Bernardo Menicagli",
  "private": true,
  "dependencies": {
    "@wojtekmaj/react-daterange-picker": "^3.4.0",
    "axios": "^0.21.1",
    "babel-plugin-formatjs": "^10.3.0",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.0",
    "chart.js": "^3.7.0",
    "chartjs-plugin-annotation": "^1.3.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.0.1",
    "react-dom": "^17.0.2",
    "react-intl": "^5.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@formatjs/cli": "^4.2.20",
    "@svgr/webpack": "^6.2.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/bootstrap": "^5.1.8",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.19",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "css-loader": "^5.2.4",
    "prettier": "2.3.1",
    "react-scripts": "5.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.5.4",
    "url-loader": "^4.1.1",
    "web-vitals": "^2.1.3",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-bundle": "webpack",
    "extract": "formatjs extract 'src/**/*.js*' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile": "formatjs compile-folder lang src/lang",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "formatjs",
        {
          "idInterpolationPattern": "[sha512:contenthash:base64:6]",
          "ast": true
        }
      ]
    ]
  }
}
